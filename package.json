{
  "name": "surf_dev_lab",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev 'src/index.ts' | pino-pretty -c",
    "lint": "eslint ./src ./__test__ --ext .ts",
    "lint:fix": "eslint ./src ./__test__ --ext .ts --fix",
    "test": "yarn lint && yarn style:check && yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./__test__ --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check 'src/**/*.ts' '__test__/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' '__test__/**/*.ts'"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Diego Gon√ßalves",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/module-alias": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.10.1",
    "@types/multer": "^1.4.4",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.7",
    "@types/pino": "^6.3.4",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "nock": "^13.0.5",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.7.1",
    "express-pino-logger": "^5.0.0",
    "express-rate-limit": "^5.2.3",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.10.15",
    "pino": "^6.7.0",
    "swagger-ui-express": "^4.1.5"
  }
}